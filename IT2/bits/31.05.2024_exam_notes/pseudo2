SET n TO 10 
SET a[0] TO 0 
SET a[1] TO 1 
FOR hvert heltall i fra og med 2 til n 
  SET a[i] TO a[i-1] + a[i-2] 
ENDFOR 
DISPLAY a[n]

2 - 10

a[2] = a[2-1] + a[2-2]
>>1
a[3] = a[3-1] + a[3-2]
>> 3
a[4] = a[4-1] + a[4-2]
>> 5
a[5]
>> 7
a[6]
>>9
a[7]
>>11
a[8]
13
a[9]
>> 15
a[10]
>> 17


[0,1,1,3,5,7,9,11,13,15,17]




